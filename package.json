{
  "private": true,
  "name": "dumi-app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --fix --ext .ts packages",
    "format": "prettier --write \"packages/**/*.ts\" \"packages/**/*.tsx\"",
    "prepare": "husky install",
    "dumi": "dumi dev",
    "dumi-build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@umijs/plugin-sass": "^1.1.1",
    "dumi": "^2.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0",
    "terser": "^5.16.3"
  },
  "dependencies": {
    "vite": "4.1.0-beta.0",
    "typescript": "^4.9.5",
    "tailwindcss": "2.2.19",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react-ui-teaching": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13"
  }
}
