{
  "private": false,
  "name": "aone-ui",
  "scripts": {
    "test": "",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --fix --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "prepare": "husky install",
    "dumi": "dumi dev",
    "dumi-build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "create:component": "aone-ui-cli create",
    "analysis": "aone-ui-cli analysis"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@umijs/plugin-sass": "^1.1.1",
    "@vitejs/plugin-react": "^3.1.0",
    "aone-ui-cli": "^1.0.1",
    "dumi": "^2.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react": "^18.1.0",
    "react-dom": "^18.0.0",
    "terser": "^5.16.3",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@types/react": "18.2.0",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react-ui-teaching": "^1.0.1",
    "tailwindcss": "2.2.19",
    "typescript": "^4.9.5",
    "vite": "4.1.0-beta.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint"
  }
}
